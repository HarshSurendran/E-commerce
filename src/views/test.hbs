<div class="row row-cols-1 row-cols-md-3 g-4">
    {{#each data}}
        <div class="col">
            <div class="card">
                <img src="{{this.img}}" class="card-img-top" alt="...">
                <div class="card-body">
                    <h5 class="card-title">{{this.product.[0].name}}</h5>
                    <p class="card-text">Color: {{this.color.[0].color}} Only for: {{this.price}}</p>
                </div>
            </div>
        </div>        
    {{/each}}
</div>


